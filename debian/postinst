#!/bin/sh

#https://wiki.debian.org/MaintainerScripts

set -e

case "$1" in
    configure)
        #enable Cleep at startup
        /etc/cleep/cleephelper.sh enable
        ;; 

    abort-remove)
        ;; 

    abort-upgrade)
        ;; 

    *)
        echo "postinst called with unknown argument '$1'" >&2
        ;;
esac

#DEBHELPER#

exit 0

